{
  "name": "Gmail Print Page Title Rename",
  "version": "1.0",
  "description": "Gmail(G Suite)の印刷画面で、組織名部分を、メールの日時に変更。",
  "permissions": ["*://mail.google.com/mail/u/0?ik*"],
  "content_scripts": [
    {
      "matches": ["*://mail.google.com/mail/u/0?ik*"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "web_accessible_resources": ["embeded-script.js"],
  "manifest_version": 2
}
